{
  "$comment": "Branch protection configuration for cuioss repositories. Used by setup-branch-protection.py. Review count and status checks are configured interactively by the command.",
  "organization": "cuioss",
  "bypass_actor": {
    "name": "cuioss-release-bot",
    "type": "app",
    "app_id": "1195186"
  },
  "ruleset": {
    "name": "main-branch-protection",
    "target": "branch",
    "branch_pattern": "main",
    "enforcement": "active",
    "rules": {
      "require_pull_request": {
        "dismiss_stale_reviews_on_push": true,
        "require_last_push_approval": false
      },
      "require_status_checks": {
        "strict_required_status_checks_policy": false
      },
      "block_force_pushes": {
        "enabled": true
      },
      "prevent_deletion": {
        "enabled": true
      }
    }
  }
}
